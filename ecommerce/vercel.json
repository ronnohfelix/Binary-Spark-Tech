{
    "builds": [
        {
            "src": "ecommerce/ecommerce/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
        }],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "ecommerce/ecommerce/wsgi.py"
            }
        ]   
}
